{
  "name": "Inspector on acids",
  "action": {},
  "manifest_version": 3,
  "version": "0.0.1.2",
  "description": "Inspect the page, choose what you need to inspect",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "css/light.css",
        "css/dark.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "options_page": "options.html",
  "icons": {
    "128": "/icons/icon128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+U"
      }
    }
  }
}